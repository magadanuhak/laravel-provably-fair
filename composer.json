{
    "name": "magadanuhak/laravel-provably-fair",
    "description": "A Laravel package to get Provably Fair random numbers that user can verify",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Magadanuhak\\ProvablyFair\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Daniel Maga"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Magadanuhak\\ProvablyFair\\ProvablyFairServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
